model items {
  id               BigInt          @id @default(autoincrement())
  name             String          @db.VarChar(255)
  main_category_id BigInt
  main_categories  main_categories @relation(fields: [main_category_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "items_main_category_id_foreign")
  locations        locations[]
}

model jaringan_struktur_ruang_badau {
  gid    Int                      @id @default(autoincrement())
  namobj String?                  @db.VarChar(250)
  stsjrn Float?
