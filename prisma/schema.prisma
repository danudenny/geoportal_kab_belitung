model sub_locations {
  id          BigInt    @id @default(autoincrement())
  location_id BigInt
  name        String    @db.VarChar(255)
  url         String?   @db.VarChar(255)
  layer_type  String?   @db.VarChar(255)
  locations   locations @relation(fields: [location_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "sub_locations_location_id_foreign")
}

model users {
  id           Int      @id @default(autoincrement())
  fullname     String   @unique(map: "ix_users_fullname") @db.VarChar(200)
  password     String   @db.VarChar(255)
  email        String   @unique(map: "ix_users_email") @db.VarChar(150)
  is_active    Boolean
  is_superuser Boolean
  created_at   DateTime @default(now()) @db.Timestamp(6)
  updated_at   DateTime @default(now()) @db.Timestamp(6)
}
