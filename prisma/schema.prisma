generator client {
  provider   = "prisma-client-js"
  output     = "../src/generated/prisma"
  engineType = "library"
}

datasource db {
  provider = "postgresql"
}

model PolaRuangRevisiRtrw {
  gid    Int                      @id @default(autoincrement())
  namobj String?                  @db.VarChar(250)
  jnsrpr Float?
  wadmkc String?                  @db.VarChar(250)
  kkop_1 String?                  @db.VarChar(250)
  kp2b_2 String?                  @db.VarChar(250)
  krb_03 String?                  @db.VarChar(250)
  cagbud String?                  @db.VarChar(250)
  resair String?                  @db.VarChar(250)
  ksmpdn String?                  @db.VarChar(250)
  hankam String?                  @db.VarChar(250)
  kkarst String?                  @db.VarChar(250)
  ptbgmb String?                  @db.VarChar(250)
  mgrsat String?                  @db.VarChar(250)
  rdbumi String?                  @db.VarChar(250)
  remark String?                  @db.VarChar(250)
  luasha Decimal?                 @db.Decimal
  geom   Unsupported("geometry")?

  @@index([geom], type: Gist)
  @@map("pola_ruang_revisi_rtrw")
}

model base_layers {
  id   BigInt @id @default(autoincrement())
  name String @db.VarChar(255)
}

model failed_jobs {
  id         BigInt   @id @default(autoincrement())
  uuid       String   @unique(map: "failed_jobs_uuid_unique") @db.VarChar(255)
  connection String
  queue      String
  payload    String
  exception  String
  failed_at  DateTime @default(now()) @db.Timestamp(0)
}

model infrastruktur_struktur_ruang_badau {
  gid    Int                      @id @default(autoincrement())
  namobj String?                  @db.VarChar(250)
